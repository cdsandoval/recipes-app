<div class="list-recipe">
  <% @recipes.each do |key, recipe| %>
  <div class="item oh">
    <img class="image left" src="<%= recipe["url_image"] %>" width="160" height="120">
    <div class="left">
      <h1 class="title"><a href="/recipes/<%= recipe["id"] %>"><%= recipe["name"] %></a></h1>
      <ul class="features">
        <li class="feature">Quality: <%="*" * recipe["quality"]%></li>
        <li class="feature">Difficulty: <%= "*" * recipe["difficult"] %> </li>
        <li class="feature">Preparation time: <%= recipe["duration_time"]%> minutes</li>
      </ul>
    </div>
  </div>
  <% end %>
</div>